{
  "name": "desafio-backend",
  "version": "0.0.1",
  "description": "",
  "bin": "build/main.js",
  "scripts": {
    "dev": "ts-node-dev --files --respawn --ignore-watch node_modules --no-notify src/main.ts",
    "dist": "npm run build & npx pkg package.json",
    "dist:minify": "npm run build & npm run minify:folder",
    "minify:folder": "uglifyjs-folder ./build -x .js -eo ./dist",
    "dev:start": "SET NODE_ENV=development & node build/main.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "build:tsc": "tsc"
  },
  "pkg": {
    "scripts": [
      "build/**/*"
    ],
    "assets": [
      "assets/**/*"
    ],
    "targets": "",
    "outputPath": "dist"
  },
